{
  "version": "1.0.0",
  "name": "mapview",
  "private": true,
  "scripts": {
    "clean": "rimraf ./wwwroot/dist/",
    "build-dev": "webpack --env.development --config webpack.config.dev.js",
    "build-prod": "webpack --env.production --config webpack.config.prod.js",
    "build": "npm run clean && npm run build-dev && npm run build-prod",
    "test-server": "node server.js"
  },
  "dependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "core-js": "^2.5.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18",
    "axios": "^0.17.1"
  },
  "devDependencies": {
    "webpack": "^3.8.1",
    "clean-webpack-plugin": "^0.1.17",
    "typescript": "^2.6.1",
    "awesome-typescript-loader": "^3.4.0",
    "angular2-template-loader": "^0.6.2",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "json-server": "^0.12.1",
    "url-loader": "^0.6.2",
    "html-loader": "^0.5.1",
    "raw-loader": "^0.5.1",
    "clean-dir": "^1.0.3"
  },
  "-vs-binding": { "BeforeBuild": [ "build" ] }
